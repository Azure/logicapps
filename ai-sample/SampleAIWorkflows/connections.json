{
  "managedApiConnections": {},
  "serviceProviderConnections": {
    "azureaisearch": {
      "displayName": "openAI-connection",
      "parameterSetName": "ConnectionString",
      "parameterValues": {
        "searchServiceAdminKey": "@parameters('aisearch_admin_key')",
        "searchServiceEndpoint": "@parameters('aisearch_endpoint')"
      },
      "serviceProvider": {
        "id": "/serviceProviders/azureaisearch"
      }
    },
    "openai": {
      "displayName": "openai",
      "parameterSetName": "NotSpecified",
      "parameterValues": {
        "openAIEndpoint": "@parameters('openai_api_key')",
        "openAIKey": "@parameters('openai_endpoint')"
      },
      "serviceProvider": {
        "id": "/serviceProviders/openai"
      }
    }
  }
}
