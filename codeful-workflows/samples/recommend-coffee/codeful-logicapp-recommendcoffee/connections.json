{
  "agentConnections": {},
  "managedApiConnections": {
    "office365": {
      "api": {
        "id": "@appsetting('office365-api-id')"
      },
      "connection": {
        "id": "@appsetting('office365-connection-id')"
      },
      "connectionRuntimeUrl": "@appsetting('office365-connection-runtime-url')",
      "authentication": {
        "type": "Raw",
        "scheme": "Key",
        "parameter": "@appsetting('office365-connectionKey')"
      }
    }
  },
  "serviceProviderConnections": {
    "azureaisearch": {
      "displayName": "aisearch-connection",
      "parameterSetName": "ConnectionString",
      "parameterValues": {
        "searchServiceAdminKey": "@appsetting('azure-aisearch-admin-key')",
        "searchServiceEndpoint": "@appsetting('azure-aisearch-endpoint')"
      },
      "serviceProvider": {
        "id": "/serviceProviders/azureaisearch"
      }
    },
    "openai": {
      "displayName": "aoai-connection",
      "parameterSetName": "KeyAndEndpointConnection",
      "parameterValues": {
        "openAIEndpoint": "@appsetting('openai-endpoint')",
        "openAIKey": "@appsetting('openai-key')"
      },
      "serviceProvider": {
        "id": "/serviceProviders/openai"
      }
    }
  }
}
