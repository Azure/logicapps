{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "generateDebugSymbols",
			"command": "dotnet",
			"args": [
				"${input:getDebugSymbolDll}"
			],
			"type": "process",
			"problemMatcher": "$msCompile"
		},
		{
			"type": "func",
			"command": "host start",
			"problemMatcher": "$func-watch",
			"isBackground": true,
			"dependsOn": "generateDebugSymbols"
		}
	],
	"inputs": [
		{
			"id": "getDebugSymbolDll",
			"type": "command",
			"command": "azureLogicAppsV2.getDebugSymbolDll"
		}
	]
}