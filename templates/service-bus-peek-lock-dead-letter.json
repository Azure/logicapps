                    "inputs": {
                        "host": {
                            "connection": {
                                "name": "@parameters('$connections')['servicebus']['connectionId']"
                            }
                        },
                        "method": "get",
                        "path": "//messages/head/peek",
                        "queries": {
                            "queueType": "Main"
                        }
                    },
                    "recurrence": {
                        "frequency": "Minute",
