            "actions": {
                "Complete_the_message_in_a_queue": {
                    "inputs": {
                        "host": {
                            "connection": {
                                "name": "@parameters('$connections')['servicebus']['connectionId']"
                            }
                        },
                        "method": "delete",
                        "path": "//messages/complete",
                        "queries": {
                            "lockToken": "@{triggerBody()?['LockToken']}",
                            "queueType": "Main"
                        }
                    },
                    "runAfter": {
                        "Scope": [
                            "Succeeded"
                        ]
                    },
                    "type": "ApiConnection"
                },
                "Dead-letter_the_message_in_a_queue": {
                    "inputs": {
                        "host": {
                            "connection": {
                                "name": "@parameters('$connections')['servicebus']['connectionId']"
                            }
                        },
                        "method": "post",
                        "path": "//messages/deadletter",
                        "queries": {
                            "lockToken": "@{triggerBody()?['LockToken']}"
                        }
                    },
                    "runAfter": {
                        "Scope": [
                            "Failed"
                        ]
                    },
                    "type": "ApiConnection"
                },
                "Scope": {
